<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.MoviceDoc" table="MoviceDoc" schema="dbo" catalog="demo">
        <id name="did">
            <column name="Did" sql-type="int"/>
        </id>
        <property name="dinfo">
            <column name="Dinfo" sql-type="varchar(500)" length="500" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>